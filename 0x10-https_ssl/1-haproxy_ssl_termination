defaults
    mode http
    timeout client 15s
    timeout connect 10s
    timeout server 15s
    timeout http-request 10s

frontend iamitsfoss-frontend
    bind *:80
    bind *:443 ssl crt /etc/haproxy/certs/iamitsfoss.tech.pem
    default_backend iamitsfoss-backend

backend iamitsfoss-backen
    balance roundrobin
    server 130022-web-01 100.25.33.199:80 check
    server 130022-web-02 54.160.100.82:80 check
